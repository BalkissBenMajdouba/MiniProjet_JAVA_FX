<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="356.0" prefWidth="687.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DisplayEnsController">

    <!-- Define the Dubai font -->
    <fx:define>
        <Font fx:id="dubaiFont" name="Dubai" size="14" />
    </fx:define>

    <children>
        <TableView fx:id="tableView" layoutX="27.0" layoutY="72.0" prefHeight="266.0" prefWidth="642.0" style="-fx-font-family: 'Dubai';">
            <columns>
                <TableColumn fx:id="idColumn" style="-fx-font-family: 'Dubai';" text="ID" /> <!-- Apply Dubai font to TableColumn -->
                <TableColumn fx:id="nameColumn" style="-fx-font-family: 'Dubai';" text="Name" /> <!-- Apply Dubai font to TableColumn -->
                <TableColumn fx:id="prenomColumn" style="-fx-font-family: 'Dubai';" text="Prenom" /> <!-- Apply Dubai font to TableColumn -->
                <TableColumn fx:id="numMobColumn" style="-fx-font-family: 'Dubai';" text="Mobile" /> <!-- Apply Dubai font to TableColumn -->
                <TableColumn fx:id="emailColumn" style="-fx-font-family: 'Dubai';" text="Email" /> <!-- Apply Dubai font to TableColumn -->
                <TableColumn fx:id="departementColumn" style="-fx-font-family: 'Dubai';" text="Departement" /> <!-- Apply Dubai font to TableColumn -->
                <TableColumn fx:id="updateColumn" style="-fx-font-family: 'Dubai';" text="Update" /> <!-- Apply Dubai font to TableColumn -->
                <TableColumn fx:id="deleteColumn" style="-fx-font-family: 'Dubai';" text="Delete" /> <!-- Apply Dubai font to TableColumn -->
            </columns>
        </TableView>
      <Text fill="#a62626" layoutX="258.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="List Enseignant">
         <font>
            <Font name="Dubai Bold" size="26.0" />
         </font>
      </Text>
      <Button fx:id="returnButton" layoutX="27.0" layoutY="21.0" onAction="#returnToAdminHome" prefHeight="35.0" prefWidth="50.0" style="-fx-font-family: 'Dubai'; -fx-background-color: #a62626; -fx-text-fill: white;" text="Return" />
    </children>
</AnchorPane>
